{
  "name": "yufka",
  "version": "4.0.0",
  "description": "Transform JavaScript ASTs the easy way",
  "keywords": [
    "ast",
    "falafel",
    "burrito",
    "source",
    "syntax",
    "traversal",
    "tree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Loilo/yufka.git"
  },
  "license": "MIT",
  "author": {
    "name": "Florian Reuschel",
    "email": "florian@loilo.de",
    "url": "https://loilo.de"
  },
  "files": [
    "dist"
  ],
  "main": "dist/cjs/yufka.common.js",
  "module": "dist/esm/yufka.esm.js",
  "scripts": {
    "build:esm": "tsc -p build/tsconfig.esm.json",
    "build:cjs": "tsc -p build/tsconfig.cjs.json",
    "build": "run-p build:*",
    "lint:tests": "eslint \"test/**/*.js\"",
    "lint:source": "eslint \"src/**/*.ts\"",
    "lint": "run-p lint:*",
    "jest": "jest",
    "test": "npm run lint && npm run build && npm run jest",
    "prepublishOnly": "npm test"
  },
  "dependencies": {
    "acorn": "^8.3.0",
    "magic-string": "^0.30.2"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^7.1.0",
    "acorn-jsx": "^5.0.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
